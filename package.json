{
  "scripts": {
    "build": "turbo build",
    "dev": "dotenv -- turbo dev",
    "dev:packages": "dotenv -- turbo run dev --filter='!./apps/*'",
    "lint": "turbo lint",
    "clean": "turbo clean",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@commitlint/types": "^17.4.4",
    "@types/node": "^20.2.4",
    "concurrently": "^8.0.1",
    "dotenv-cli": "^7.2.1",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.11",
    "turbo": "^1.9.3"
  },
  "packageManager": "npm@9.5.1",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
