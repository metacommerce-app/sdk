{
  "name": "@metacommerce-app/minting",
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist",
    "./styles.css": "./dist/index.css"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup && tailwindcss -i ./src/styles.css -o ./dist/styles.css",
    "dev": "concurrently \"npm run dev:*\"",
    "dev:tailwind": "tailwindcss -i ./src/styles.css -o ./dist/styles.css --watch",
    "dev:tsup": "DEV=true tsup --watch",
    "check-types": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.5",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "postcss": "^8.4.20",
    "react": "^18.2.0",
    "tailwind-config": "*",
    "tsconfig": "*",
    "tsup": "^6.1.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@ant-design/icons": "^5.1.3",
    "@tanstack/react-query": "^4.29.7",
    "@wagmi/chains": "^0.3.1",
    "@wagmi/core": "^1.0.7",
    "ethers": "^6.4.0",
    "lottie-web": "^5.11.0",
    "pino": "^8.14.1",
    "viem": "^0.3.37"
  }
}
